{
	"search.exclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"**/built/**": true
	},
	"typescript.tsdk": "node_modules/typescript/lib",
	"files.associations": {
		"*.test.ts": "typescript"
	},
	"search.followSymlinks": false,
	"editor.defaultFormatter": "biomejs.biome",
	"editor.formatOnSave": false
}
