html

	head
		meta(charset='utf-8')
		meta(name='application-name' content='Tanukey')
		meta(name='referrer' content='origin')
		meta(name='theme-color' content= themeColor || '#86b300')
		meta(name='theme-color-orig' content= themeColor || '#86b300')
		if siteGtagGoogleAnalytics
			meta(name='siteGtagGoogleAnalytics' content= siteGtagGoogleAnalytics)
		if siteGtagGoogleAds
			meta(name='siteGtagGoogleAds' content= siteGtagGoogleAds)
			meta(name='siteGtagGoogleAdsConversion' content= siteGtagGoogleAdsConversion)
		meta(property='og:site_name' content= instanceName || 'Tanukey')
		meta(name='viewport' content='width=device-width, initial-scale=1')
		link(rel='icon' href= icon || '/favicon.ico')
		link(rel='apple-touch-icon' href= icon || '/apple-touch-icon.png')
		link(rel='manifest' href='/manifest.json')
		link(rel='search' type='application/opensearchdescription+xml' title=(title || "Tanukey") href=`${url}/opensearch.xml`)
		link(rel='prefetch' href=serverErrorImageUrl)
		link(rel='prefetch' href=infoImageUrl)
		link(rel='prefetch' href=notFoundImageUrl)
		//- https://github.com/misskey-dev/misskey/issues/9842
		link(rel='stylesheet' href='/assets/tabler-icons/tabler-icons.min.css?v2.21.0')
		link(rel='modulepreload' href=`/vite/${clientEntry.file}`)

		if !config.clientManifestExists
				script(type="module" src="/vite/@vite/client")

		if Array.isArray(clientEntry.css)
			each href in clientEntry.css
				link(rel='stylesheet' href=`/vite/${href}`)

		title
			block title
				= title || 'Tanukey'

		block desc
			meta(name='description' content= desc || '‚ú®üåé‚ú® A interplanetary communication platform ‚ú®üöÄ‚ú®')

		block meta

		block og
			meta(property='og:title'       content= title || 'Tanukey') 
			meta(property='og:description' content= desc || '‚ú®üåé‚ú® A interplanetary communication platform ‚ú®üöÄ‚ú®') 
			meta(property='og:image'       content= img)
			meta(property='twitter:card'   content='summary')

		style
			include ../style.css

		script.
			var VERSION = "#{version}";
			var CLIENT_ENTRY = "#{clientEntry.file}";

		<!-- Google tag (gtag.js) -->
		if siteGtagGoogleAnalytics
			script(async src= 'https://www.googletagmanager.com/gtag/js?id=' + siteGtagGoogleAnalytics)
		if siteGtagGoogleAds
			script(async src= 'https://www.googletagmanager.com/gtag/js?id=' + siteGtagGoogleAds)
		if siteGtagGoogleAnalytics || siteGtagGoogleAds
			script.
				window.dataLayer = window.dataLayer || [];
				function gtag(){dataLayer.push(arguments);}
				gtag('js', new Date());
				if ("#{siteGtagGoogleAnalytics}" !== "") {
					gtag('config', "#{siteGtagGoogleAnalytics}");
				}
				if ("#{siteGtagGoogleAds}" !== "") {
					gtag('config', "#{siteGtagGoogleAds}");
				}

		script
			include ../boot.js

